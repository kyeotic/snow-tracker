{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "NODE_ENV=production run-s build:dev build:prod",
    "build:prod": "tsc --project ./tsconfig.prod.json",
    "build:dev": "remix build",
    "dev": "npm-run-all build:dev --parallel \"dev:*\"",
    "dev:node": "cross-env NODE_ENV=development nodemon --require dotenv/config ./server.ts --watch ./server.ts",
    "dev:remix": "remix watch",
    "worker:dev": "nodemon --require dotenv/config ./worker/debug.ts",
    "start": "cross-env NODE_ENV=production ts-node ./server.js",
    "typecheck": "tsc",
    "style": "prettier --write \"{app}/**/*.{js,ts,tsx}\""
  },
  "dependencies": {
    "@remix-run/express": "^1.14.1",
    "@remix-run/node": "^1.14.1",
    "@remix-run/react": "^1.14.1",
    "cheerio": "^1.0.0-rc.12",
    "compression": "^1.7.4",
    "croner": "^6.0.3",
    "cross-env": "^7.0.3",
    "express": "^4.18.2",
    "isbot": "^3.6.5",
    "luxon": "^3.3.0",
    "morgan": "^1.10.0",
    "node-schedule": "^2.1.1",
    "nodemon": "^2.0.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-loading-icons": "^1.1.0",
    "react-simple-pull-to-refresh": "^1.3.3",
    "request-micro": "^2.0.1",
    "url-join": "^4.0.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.14.1",
    "@remix-run/eslint-config": "^1.14.1",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.17",
    "@types/luxon": "^3.2.0",
    "@types/morgan": "^1.9.4",
    "@types/node-schedule": "^2.1.0",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@types/url-join": "^4.0.1",
    "dotenv": "^16.0.3",
    "eslint": "^8.27.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">=14"
  }
}
